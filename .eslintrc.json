{
  "plugins": ["jsx-a11y"],
  "extends": [
    "plugin:jsx-a11y/recommended",
    "eslint:recommended",
    "plugin:promise/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/click-events-have-key-events": "off"
  },
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 13
  }
}